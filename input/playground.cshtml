Title: Playgound
---
<section class="bg-main py-5">
    <div class="container">
        <h1>@Document.GetString("Title")</h1>
        <div class="row g-0 h-100-px">
            <div class="col bg-alpha">Hi.</div>
            <div class="col bg-beta">Hi.</div>
            <div class="col bg-gamma">Hi.</div>
            <div class="col bg-delta">Hi.</div>
            <div class="col bg-epsilon">Hi.</div>
            <div class="col bg-zeta">Hi.</div>
            <div class="col bg-main">Hi.</div>
            <div class="col bg-opposite">Hi.</div>
        </div>
        <div class="row g-0 h-100-px">
            <div class="col bg-pink">Hi.</div>
            <div class="col bg-red">Hi.</div>
            <div class="col bg-orange">Hi.</div>
            <div class="col bg-yellow">Hi.</div>
            <div class="col bg-green">Hi.</div>
            <div class="col bg-cyan">Hi.</div>
            <div class="col bg-blue">Hi.</div>
            <div class="col bg-purple">Hi.</div>
            <div class="col bg-white">Hi.</div>
            <div class="col bg-gray">Hi.</div>
            <div class="col bg-black">Hi.</div>
        </div>
        <ul class="list-unstyled list-inline">
            <li class="list-inline-item">
                <a class="icon icon-circle icon-twitter" href="#">
                    <i class="fa-brands fa-twitter"></i>
                </a>
            </li>
            <li class="list-inline-item position-relative">
                <a class="icon icon-circle icon-blip icon-blip-facebook" href="#">
                    <i class="fa-brands fa-facebook-f"></i>
                </a>
            </li>
        </ul>
        <div class="row mt-5">
            <div class="col-sm-4">
                <svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100%">
                    <pattern id="svgCrossHairs" x="0" y="0" width="40" height="40" patternUnits="userSpaceOnUse">
                        <rect x="0" y="0" width="100%" height="100%" fill="var(--sh-main)"/>
                        <path d="M 20,-2.55e-7 V 40 Z M -1.1677362e-8,20 H 40 Z"  stroke-width="1" stroke="var(--sh-opposite)" fill="none"/>
                    </pattern>
                    <rect fill="url(#svgCrossHairs)" stroke="var(--sh-alpha)" width="100%" height="100%"/>
                </svg>
            </div>
            <div class="col-sm-4">
                <svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100%">
                    <pattern id="svgCrossHairs" x="0" y="0" width="40" height="40" patternUnits="userSpaceOnUse">
                        <rect x="0" y="0" width="100%" height="100%" fill="var(--sh-main)"/>
                        <path d="M 20,-2.55e-7 V 40 Z M -1.1677362e-8,20 H 40 Z"  stroke-width="1" stroke="var(--sh-opposite)" fill="none"/>
                    </pattern>
                    <rect fill="url(#svgCrossHairs)" stroke="var(--sh-alpha)" width="100%" height="100%"/>
                </svg>
            </div>
        </div>
    </div>
</section>
