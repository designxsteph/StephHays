        <a class="link-underline link-underline-black mb-5 h3 d-inline-block" href='@Context.GetLink("/blog")' >Read More Blogs</a>
        <div class="row">
            @{
                IDocument[] recentBlogs = Outputs
                    .FromPipeline("Content")
                    .FilterSources("archive/*.md")
                    .Where(x => x.GetString("Type").Equals("blog"))
                    .OrderByDescending(x => x.GetDateTime(WebKeys.Published))
                    .Take(3)
                    .ToArray();
            }

            @foreach (IDocument document in recentBlogs)
            {
                @Html.Partial("_PostBlurb", (IDocument) document)
            }
        </div>
    </div>
</section>